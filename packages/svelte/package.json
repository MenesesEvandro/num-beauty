{
    "name": "@num-beauty/svelte",
    "version": "0.1.0",
    "type": "module",
    "description": "Svelte integration for num-beauty",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "svelte-package -i src/lib -o dist",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "lint": "eslint \"src/**/*.{ts,svelte}\" --fix",
        "clean": "rm -rf dist",
        "test": "vitest run"
    },
    "keywords": [
        "number",
        "formatting",
        "svelte",
        "component",
        "action",
        "store"
    ],
    "author": "Evandro Meneses",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MenesesEvandro/num-beauty.git",
        "directory": "packages/svelte"
    },
    "peerDependencies": {
        "svelte": "^4.0.0 || ^5.0.0"
    },
    "dependencies": {
        "num-beauty": "workspace:*"
    },
    "devDependencies": {
        "@sveltejs/package": "^2.2.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@testing-library/svelte": "^5.2.9",
        "jsdom": "^27.2.0",
        "svelte": "^4.2.0",
        "svelte-check": "^3.6.0",
        "tslib": "^2.6.0",
        "typescript": "^5.8.0",
        "vite": "^5.0.0",
        "vitest": "^4.0.14"
    },
    "sideEffects": false
}